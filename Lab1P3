import java.until.Scanner;
class Lab1P3{
      public static void main(String...args){
       Scanner s = new Scanner(System.in);
       try{
              int a = s.nextInt();
              int b = s.nextLine();
              //var c a/b;
              System.out.println(a/b);
      }catch(exception e){
       Sytem.out.println(e.getClass());
       }
     }
  }
      


